    <DataSet Name="YearFilter">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with yearlist as 
(
select 2000 as year
union all
select yl.year + 1 as year
from yearlist yl
where yl.year + 1 &lt;= YEAR(GetDate())
)

select year
from yearlist 
order by year desc;</CommandText>
      </Query>
