  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@month">
            <Value>=Parameters!month.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select temp.[Transaction Year]
,temp.[Transaction Month Name]
,temp.short_description_l
,sum(temp.[count]) 
from 
(
		select year(pvdv.recorded_at_date_time) as [Transaction Year], 
				--month(pvdv.recorded_at_date_time) as [Transaction Month], 
				datename(month,pvdv.recorded_at_date_time) as [Transaction Month Name],
				pvcdv.short_description_l,
				COUNT(pvdv.patient_visit_id) as [count]
		from patient_visit_diagnosis_view as pvdv inner join patient_visit_current_primary_diagnosis_view as pvcdv on pvcdv.patient_visit_id = pvdv.patient_visit_id
		inner join patient_visit pv on pv.patient_visit_id = pvdv.patient_visit_id
		where (year(pvdv.recorded_at_date_time) = convert(int,@year)
or year(pvdv.recorded_at_date_time) = convert(int,@year) -1) 
and (month(pvdv.recorded_at_date_time) = @month)
and (pvdv.diagnosis_type_rcd = 'dis')
and (pvdv.coding_type_rcd = 'pri')
and (pvdv.coding_system_rcd = 'icd10')
and (pv.cancelled_date_time is null)
and (pv.charge_type_rcd = 'ipd')
and (pvcdv.patient_medical_coding_type_rcd = 'visit')
and (pvdv.active_flag = '1')
and (pvdv.primary_flag = '1')
and (pvdv.current_visit_diagnosis_flag = '1')
--and (pvcdv.short_description_l like '%Pneumonia%')
		group by pvcdv.short_description_l,pvdv.recorded_at_date_time
		--order by Count(pvdv.patient_visit_id) desc
		)
		as temp
		group by temp.[Transaction Year],temp.[Transaction Month Name],temp.count,temp.short_description_l
		order by temp.count</CommandText>
      </Query>
      <Fields>
        <Field Name="Transaction_Year">
          <DataField>Transaction Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Transaction_Month_Name">
          <DataField>Transaction Month Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="short_description_l">
          <DataField>short_description_l</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField />
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>; with  TestYear as
        (
        select  convert(varchar(4),datepart(year, '2003-01-01')) as yr
        union all
        select  convert(varchar(4),yr + 1)
        from    TestYear
        where   yr &lt; datepart(year, GETDATE())
        )
select  yr
from    TestYear
order by yr desc</CommandText>
      </Query>
      <Fields>
        <Field Name="yr">
          <DataField>yr</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
